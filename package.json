{
  "name": "devbook",
  "description": "Search engine for developers",
  "version": "0.1.14",
  "private": true,
  "homepage": ".",
  "author": {
    "name": "FoundryLabs, Inc.",
    "email": "hello@usedevbook.com",
    "url": "https://usedevbook.com"
  },
  "main": "build/main",
  "productName": "Devbook",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "compile": "craco build && tsc -p src/main",
    "postinstall": "electron-rebuild",
    "electron:dev": "concurrently \"npx cross-env BROWSER=none craco start\" \"wait-on http://localhost:3000 && tsc -p src/main -w\" \"wait-on http://localhost:3000 && tsc -p src/main && electron --enable-logging .\"",
    "build:prod": "npm run compile && todesktop build",
    "build:dev": "npm run compile && todesktop build --code-sign=false"
  },
  "dependencies": {
    "@sentry/electron": "^2.4.0",
    "@todesktop/runtime": "^0.4.0",
    "analytics-node": "^3.5.0",
    "auto-launch": "^5.0.5",
    "axios": "^0.21.1",
    "debounce": "^1.2.0",
    "electron-context-menu": "^2.5.0",
    "electron-store": "^5.2.0",
    "magic-sdk": "^4.0.2",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "re-resizable": "^6.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hotkeys-hook": "^2.4.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@sentry/webpack-plugin": "^1.14.2",
    "@types/analytics-node": "^3.1.4",
    "@types/auto-launch": "^5.0.1",
    "@types/debounce": "^1.2.0",
    "@types/node": "^12.12.6",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.0",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.15.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.3",
    "electron": "11.1.1",
    "electron-rebuild": "^2.3.5",
    "electron-reload": "^1.5.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.0.2",
    "wait-on": "^5.2.1"
  }
}
